in_pin: none
out_pin: none

@serialize
file_var(0,chan);
chan = file_mem(0,0,0);

@init
CC_NUM = 32; //Uacc number
NUM_SLIDERS = 16;

@slider

uacc = 50; //Start at mem position 50
uacc[0] = slider1;
uacc[1] = slider2;
uacc[2] = slider3;
uacc[3] = slider4;
uacc[4] = slider5;
uacc[5] = slider6;
uacc[6] = slider7;
uacc[7] = slider8;
uacc[8] = slider9;
uacc[9] = slider10;
uacc[10] = slider11;
uacc[11] = slider12;
uacc[12] = slider13;
uacc[13] = slider14;
uacc[14] = slider15;
uacc[15] = slider16;

@block

  while (midirecv(offset, msg1, msg2, msg3))
  (
    //Extract message type
    status = msg1 & $xF0;

    //Is it a controller event?
    status == $xB0 ?
    (
        //Is it the right CC?
        msg2 == CC_NUM ?
        (
          chan = -1; //Reset
          msg3 != 0 ? //CC value of 0 = disabled
          (
             i = 0;
              loop(NUM_SLIDERS-1, //Each slider/channel
                uacc[i] == msg3 ?
                  (
                    chan = i; //Store the output channel
                    i = NUM_SLIDERS //Exit the loop
                  );
                i += 1;
              );
            );
        );
    );

  chan != -1 ?
  (
    midisend(offset, msg1+chan, msg2, msg3); //Forward to channel
  ):(
    midisend(offset, msg1, msg2, msg3); // Pass thru
  )
  );
